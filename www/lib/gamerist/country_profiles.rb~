module Gamerist
  def country(code)
    case code
    when :EST
      return {
        vat: .20,
        paypalcurrency: :EUR,
        masspaycurrency: :EUR,
        masspayrate: .02,
        masspayfallout: 6.
      }
    else
      return {
        vat: 1.20,
        paypalcurrency: :EUR,
        masspaycurrency: :EUR,
        masspayrate: .02,
        masspayfallout: 35.
      }
    end
  end
end
